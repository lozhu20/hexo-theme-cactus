<footer id="footer">
  <div class="footer-left">
    <!-- <%= __('footer.copyright') %> --> &copy;
    <% var endYear = (theme.copyright && theme.copyright.end_year) ? theme.copyright.end_year : new Date().getFullYear() %>
    <% var startYear = (theme.copyright && theme.copyright.start_year) ? theme.copyright.start_year : new Date().getFullYear() %>
    <%= startYear >= endYear ? endYear : startYear + "-" + endYear %>
    <%= config.author || config.title %>
  </div>
  <div class="footer-center">
    <% if (theme.moe_icp.enabled && theme.moe_icp.number) { %>
      <a href="https://icp.gov.moe/?keyword=<%= theme.moe_icp.number %>" target="_blank">萌ICP备<%= theme.moe_icp.number %>号</a>
    <% } %>
    <% if (theme.travellings.enabled) { %>
      <% if (theme.moe_icp.enabled && theme.moe_icp.number) { %>
        <span> | </span>
      <% } %>
      <a href="<%= theme.travellings.url %>" target="_blank"><%= theme.travellings.description %></a>
    <% } %>
    <% if (theme.forever_blog.enabled) { %>
      <% if (theme.travellings.enabled) { %>
        <span> | </span>
      <% } %>
      <a href="<%= theme.forever_blog.url %>" target="_blank"><%= theme.forever_blog.description %></a>
    <% } %>
  </div>
  <div class="footer-right">
    <nav>
      <ul>
        <% for (var i in theme.nav) { %>
          <% if (theme.nav[i].indexOf("theme") < 0) { %>
            <a href="<%- url_for(theme.nav[i]) %>"><%= __('nav.'+i).replace("nav.", "") %></a>
          <% } %>
        <% } %>
      </ul>
    </nav>
  </div>
</footer>
